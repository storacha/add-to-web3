{
  "name": "add-to-web3",
  "version": "1.0.0",
  "description": "Publish your files and websites to web3.storage",
  "main": "index.js",
  "author": "olizilla",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/web3-storage/add-to-web3.git"
  },
  "keywords": [
    "web3",
    "storage",
    "IPFS",
    "Filecoin"
  ],
  "scripts": {
    "test": "ava --verbose",
    "lint": "standard --verbose | snazzy",
    "lint:fix": "standard --fix",
    "prepare": "ncc build index.js -o dist --source-map --license licenses.txt",
    "all": "npm run lint && npm run prepare && npm test"
  },
  "dependencies": {
    "@actions/core": "^1.4.0",
    "files-from-path": "^0.1.3",
    "web3.storage": "^2.1.1"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.29.0",
    "ava": "^3.15.0",
    "snazzy": "^9.0.0",
    "standard": "^16.0.3"
  },
  "standard": {
    "ignore": [
      "/dist"
    ]
  }
}
